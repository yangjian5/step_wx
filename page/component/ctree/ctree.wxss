@import '../../common/common.wxss';

.header{
    position: relative;
    height: 160rpx;
    line-height: 100rpx;
    padding:30rpx 30rpx 30rpx 150rpx;
    box-sizing: border-box;
    background: #AB956D;
    font-size: 28rpx;
    color: #fff;
}
.header .thumb{
    position: absolute;
    left: 30rpx;
    top: 30rpx;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}
.header .about{
    float: right;
}

.img-enter {
  display: flex;
  align-items: center;
  justify-content: center;  
  width: 100%;
  height: 360rpx;
  margin-top: 0rpx;
  margin-bottom: 26rpx;
}

.btn {
  width: 50%;
  margin-top: 30rpx;
  background-color: orange
}


